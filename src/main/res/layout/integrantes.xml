<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#ffebdfa1"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".Integrants">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/isaludable"
        android:contentDescription="isaludable" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Nombre:"
        android:id="@+id/textView"
        android:textColor="#ffa06455"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="25dp"
        android:layout_marginTop="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Fecha de Nacimiento:"
        android:id="@+id/textView1"
        android:textColor="#ffa06455"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="25dp"
        android:layout_marginTop="65dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Sexo:"
        android:id="@+id/textView2"
        android:textColor="#ffa06455"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="25dp"
        android:layout_marginTop="125dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Sexo:"
        android:id="@+id/textView3"
        android:textColor="#ffa06455"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="25dp"
        android:layout_marginTop="195dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:gravity="right"
        android:id="@+id/editTextName"
        android:inputType="none"
        android:maxLength="20"
        android:layout_alignBottom="@+id/textView"
        android:layout_toRightOf="@+id/textView"
        android:background="#ffffff"
        android:layout_toEndOf="@+id/textView" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:gravity="right"
        android:id="@+id/editTextBirth"
        android:inputType="none"
        android:maxLength="20"
        android:background="#ffffff"
        android:layout_below="@+id/textView1" />

    <EditText
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:gravity="right"
        android:id="@+id/editTextSex"
        android:inputType="none"
        android:maxLength="20"
        android:background="#ffffff"
        android:layout_below="@+id/textView2" />





</RelativeLayout>